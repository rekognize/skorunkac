#: skorunkac/polls/templates/result.html:66
msgid "Your score by categories"
msgstr "Kategorilere göre skorun"

#: skorunkac/polls/templates/result.html:88
msgid "Sorry, you couldn’t obtain a good score in any of the categories."
msgstr "Maalesef hiçbir kategoriden iyi puan alamadın."

#: skorunkac/polls/templates/result.html:93
#, python-format
msgid ""
"\n"
"                    Your best category is <em>%(category)s</em>;\n"
"                    your score in this category is <span class=\"text-success"
"\">%(score)s</span>\n"
"                "
msgstr ""
"En iyi olduğun kategori <em>%(category)s</em>;bu kategorideki skorun <span "
"class=\"text-success\">%(score)s</span>"

#: skorunkac/polls/templates/result.html:106
